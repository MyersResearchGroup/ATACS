[ aro: (~pri & pro)] Combinational
[+breq: (pri & ~busy)]
[-breq: (bprn & ~pri & busy)]
[+busy: (breq & mrdc & ~x)]
[-busy: (x)]
[+do: (~mrdc & ~x)]
[-do: (mrdc & ~pdo)]
[+mrdc: (din)]
[-mrdc: (~din & xack & ~busy & x)]
[+pdo: (~din & ~pack)]
[-pdo: (din & pack)]
[+pro: (~ari & breq)]
[-pro: (ari & ~breq)]
[+x: (~bprn & ~breq & busy)]
[-x: (~xack & ~mrdc & pdo)]
